[core]
	excludesfile = ~/.config/git/.gitignore
	quotepath = false
	editor = vim
[alias]
	lol = log --oneline
	s = status -uall
	ss = status -s -uall
	ssb = status -sb -uall
	cm = commit -m
  cv = commit -v
	cam = commit --amend -m
	df = diff
  co = checkout
